#include <bits/stdc++.h>
#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
using namespace std;
typedef long long ll;
string xau(ll n,ll k)
{
    string A="abcd";
    string kq="";
    k--;
    for(int i=0;i<n;++i)
        {
            ll dem=k/(ll)pow(4,n-i-1);
            kq+=A[dem];
            k%=(ll)pow(4,n-i-1);
        }
    return kq;
}
ll aa(ll n)
{
    ll S[n]={};
    S[0]=1;
    for (ll i=1;i<=n;i++)
        {
            S[i]=(S[i-1]*4)%9901;//cout<<S[i]<<" ";
            
        }
    return S[n];
}
int main() {
    ll n=1000;
    cout<<"a1000 mod 9901 = "<<aa(n)<<endl;
    ll k=100;
    ll m=10;
    string s=xau(m,k);
    cout<<"phan tu thu 100 cua s10 = "<<s<< endl;
    return 0;
}
