#include <bits/stdc++.h>
#include <iostream>
#include <string>
#include <algorithm>
#include <cmath>
using namespace std;
typedef long long ll;
ll aa(ll n)
{
    ll S[n]={};
    S[0]=1;
    for (ll i=1;i<=n;i++)
        {
                S[i]=(3*S[i-1]+3*S[i-2])%9901;

            //cout<<S[i]<<" ";
        }
    return S[n];
}
int main() {
    ll n=1000;
    cout<<"b1000 mod 9901 = "<<aa(n)<<endl;
    return 0;
}
