#include <iostream>
using namespace std;

const int MOD = 9901;


long long modPow(long long a, long long b, long long m) {
    long long result = 1;
    a %= m;
    while (b > 0) {
        if (b % 2 == 1)
            result = (result * a) % m;
        a = (a * a) % m;
        b /= 2;
    }
    return result;
}

long long modInverse(long long a, long long m) {
    long long m0 = m, x0 = 0, x1 = 1;
    while (a > 1) {
        long long q = a / m;
        long long temp = m;
        m = a % m;
        a = temp;

        temp = x0;
        x0 = x1 - q * x0;
        x1 = temp;
    }
    if (x1 < 0) x1 += m0;
    return x1;
}

int main() {
    long long a = modPow(2, 1000, MOD);
    long long b = modPow(3, 1000, MOD);
    long long inv6 = modInverse(6, MOD);

    long long e1000 = (a + ((b + 3) % MOD) * inv6) % MOD;
    cout << "e1000 mod 9901 = " << e1000 << endl;

    return 0;
}
