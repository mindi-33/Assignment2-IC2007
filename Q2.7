#include <iostream>
using namespace std;

int main() {
    const int MOD = 9901;
    long long a = 1, b = 0, c = 0, d = 0;
    long long na, nb, nc, nd;

    for (int i = 0; i < 1000; i++) {
        na = (2*b - 15*c) % MOD;
        nb = (a - 15*d) % MOD;
        nc = (-5*a + 2*d) % MOD;
        nd = (c - 5*b) % MOD;

        a = (na + MOD) % MOD;
        b = (nb + MOD) % MOD;
        c = (nc + MOD) % MOD;
        d = (nd + MOD) % MOD;
    }

    long long result = (a + b + c + d) % MOD;
    cout << result << endl;
    return 0;
}
